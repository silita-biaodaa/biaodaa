<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.silita.biaodaa.dao.TbCompanyQualificationMapper" >

  <select id="queryCompanyQualification" parameterType="java.lang.String" resultType="com.silita.biaodaa.model.TbCompanyQualification">
    select * from tb_company_qualification where com_id = #{comId} GROUP BY qual_name ORDER BY valid_date DESC
  </select>

  <select id="queryCompanyQualCount" resultType="java.lang.Integer" parameterType="java.lang.String">
     select count(1) from tb_company_qualification where com_id = #{comId}
  </select>
</mapper>